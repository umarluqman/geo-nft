{
  "name": "house",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "codegen": "apollo client:codegen --target typescript --localSchemaFile schema.gql --outputFlat --includes \"{pages,src}/**\" --excludes \"src/generated/**\" src/generated",
    "codegen:watch": "yarn codegen --watch",
    "db:init": "yarn prisma init",
    "db:generate": "yarn prisma generate",
    "db:migrate:new": "yarn prisma migrate save --experimental",
    "db:migrate:up": "yarn prisma migrate up --experimental",
    "db:migrate:down": "yarn prisma migrate down --experimental",
    "compile": "npx hardhat compile",
    "testnet:deploy": "npx hardhat run scripts/deploy-world.js --network testnet",
    "test": "npx hardhat test test/test.js"
  },
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.2.0",
    "@reach/combobox": "^0.11.2",
    "autoprefixer": "^10.0.2",
    "chai": "^4.3.4",
    "class-validator": "^0.12.2",
    "deepmerge": "^4.2.2",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.0",
    "geolib": "^3.3.1",
    "hardhat": "^2.4.1",
    "ipfs-http-client": "^50.1.2",
    "next": "10.0.1",
    "postcss": "^8.1.7",
    "postcss-import": "^14.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.11.1",
    "react-map-gl": "^5.2.10",
    "redaxios": "^0.4.1",
    "reflect-metadata": "^0.1.13",
    "tailwindcss": "^2.2.6",
    "use-debounce": "^5.0.4",
    "use-google-maps-script": "^0.1.0",
    "use-places-autocomplete": "^1.6.0",
    "web3modal": "^1.9.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@prisma/cli": "~2.11.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.2.0",
    "@types/react": "^16.9.56",
    "@types/react-map-gl": "^5.2.9",
    "apollo": "^2.31.1",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "postcss-flexbugs-fixes": "^5.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "typechain": "^5.1.2",
    "typescript": "^4.0.5"
  },
  "postinstall": "typechain",
  "resolutions": {
    "graphql": "15.4"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
